            intervals [26]:
                xmin = 330.899
                xmax = 339.1
                text = " A da li postojala ka razlika, recimo, va frizuri? Dao se plela kosa kod divojčic, a dao su starije žene..."
            intervals [27]:
                xmin = 339.1
                xmax = 348.922
