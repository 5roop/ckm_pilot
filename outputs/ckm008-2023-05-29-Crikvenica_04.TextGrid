            intervals [4]:
                xmin = 41.189
                xmax = 49.339
                text = " U stare, onda je... mislin da je moj tata bil rekav da je kot da se dan sto... ali ne moren reć."
            intervals [5]:
                xmin = 57.625
                xmax = 58.975
                text = " Ča muške i ča ženske?"
            intervals [6]:
                xmin = 58.975
                xmax = 61.928
                text = " A možnje bi rekli da je neka razlika među nošnjami."
            intervals [7]:
