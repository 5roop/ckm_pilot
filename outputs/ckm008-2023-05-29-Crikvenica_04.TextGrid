            intervals [39]:
                xmin = 419.577
                xmax = 426.243
                text = " Recite mi, ki se blagdan kad ste vi bili dite najviše stova? Dalo je to bil božić, vazam ili neč..."
            intervals [40]:
                xmin = 427.458
